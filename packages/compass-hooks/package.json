{
  "name": "@xinghunm/compass-hooks",
  "version": "0.0.0",
  "description": "Common React hooks for Compass UI and other applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react,react-dom --outDir dist --clean --minify",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react,react-dom --outDir dist --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "utils"
  ],
  "author": "xinghunm",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
