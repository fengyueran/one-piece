{
  "name": "@xinghunm/compass-ui",
  "version": "0.6.0",
  "description": "Compass UI - A React component library that guides your way to better user experience",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react,react-dom --outDir dist --clean --minify",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react,react-dom --outDir dist --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "compass",
    "typescript",
    "navigation"
  ],
  "author": "xinghunm",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@floating-ui/react": "^0.27.16",
    "@xinghunm/compass-hooks": "workspace:^",
    "async-validator": "^4.2.5",
    "date-fns": "^4.1.0",
    "react-transition-group": "^4.4.5",
    "react-window": "^2.2.3"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.12",
    "@storybook/addon-links": "^9.1.12",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react": "^9.1.12",
    "@storybook/react-vite": "^9.1.12",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^8.6.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/react-transition-group": "^4.4.12",
    "@types/react-window": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.0.0",
    "prettier-v2": "npm:prettier@^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.1.12",
    "ts-jest": "^29.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
